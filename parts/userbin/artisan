if [ ! -f artisan ]; then
	if [ -d current ]; then
		echo "Lets CD to current first.. "
		cd current
		echo "Now artisan $@"
		php artisan $@
	else
		echo "No file artisan found. cd current? "
	fi
	exit
fi
